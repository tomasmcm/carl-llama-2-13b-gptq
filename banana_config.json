{
    "version": "1",
    "inputs": [
        {
            "name": "prompt",
            "description": "The prompt to feed to the model.",
            "type": "string",
            "required": true,
            "default": "hello"
        },
        {
            "name": "temperature",
            "description": "The temperature to use.",
            "type": "number",
            "default": 0.8
        },
        {
            "name": "max_new_tokens",
            "description": "Generate at most this many new tokens in the response.",
            "type": "number",
            "default": 128
        }
    ],
    "examples": [
        {
            "inputs": {
                "instruction": "hello"
            },
            "output": {
                "outputs": " Hello. How are you feeling today?"
            }
        }
    ]
}
